<%= render 'users/list', user: @user,new_book:@new_book %>

      <div class="col-xs-9">
        <h2>Users</h2>

          <table class="table table-hover table-inverse">
            <thead>
              <tr> 
                <th>image</th>
                <th>name</th>
                <th></th>
                <th colspan="3"></th>
              </tr>
            </thead>
	
	
	<tbody>
	<% @users.each do |user| %> 
		<tr>
			<td><%= attachment_image_tag user, :profile_image, :fill, 60, 60, format:'jpeg', class: "#", fallback: "no_image.jpg", size:'60x60' %></td>
			<td><%= user.name %></td> 
			<td><%= link_to "Show",user_path(user) %></td>
		</tr>
	<% end %>
	</tbody>
</table>
</div>
